<a href="{{ asset('img/15_ekosystem.png') }}" class="image-link">
    <img src="{{ asset('img/15_ekosystem.png') }}" alt="">
    </a>

<div id="graph">
</div>
    
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Year</th>
            <th>Hänsynskravande Biotoper</th>
